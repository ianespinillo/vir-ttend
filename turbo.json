{
	"$schema": "https://turbo.build/schema.json",
	"tasks": {
		"build": {
			"dependsOn": ["^build"],
			"outputs": [".next/**", "!.next/cache/**", "dist/**"]
		},
		"check-types": {
			"dependsOn": ["^check-types", "^build"],
			"persistent": true,
			"cache": false
		},
		"dev": {
			"persistent": true,
			"cache": false
		},
		"db:generate": {
			"cache": false
		},
		"db:push": {
			"cache": false
		},
		"db:seed": {
			"cache": false
		},
		"lint": {
			"cache": false
		},
		"test": {
			"cache": false
		},
		"prepare": {
			"cache": false
		},
		"ts:check": {
			"cache": false
		}
	}
}
